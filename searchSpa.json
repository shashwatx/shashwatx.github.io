---
layout: none
---
[
    {% for post in site.posts %}
        {% if post.type == "" or post.type == nil %}
            {% if post.spice == "" or post.spice == nil %}
                {
                "title"    : "{{ post.title | escape }}",
                "url"      : "{{ site.baseurl }}{{ post.url }}",
                "date"     : "{{ post.date }}"
                }
            {% unless forloop.last %},{% endunless %}
        {% endif %}
        {% endif %}
	{% endfor %}
]
